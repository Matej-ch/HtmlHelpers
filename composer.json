{
  "name": "matejch/html_helpers",
  "description": "Helper class for removing elements and content, and extracting file paths",
  "version": "1.0.1",
  "keywords": ["html","file extraction","tag remover"],
  "license": "MIT",
  "authors": [
    {
      "name": "Matej Ch",
      "email": "matejCH.work@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2.0",
    "ext-dom": "*"
  },
  "autoload": {
    "psr-4": {
      "matejch\\html\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "matejch\\html\\tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5"
  },
  "scripts": {
    "test": "vendor/bin/phpunit tests/"
  }
}